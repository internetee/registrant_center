{
    "name": "eis-kliendiportaal",
    "version": "1.0.0",
    "description": "Estonian Internet Foundation Client App",
    "author": "Web Expert OÃœ",
    "engines": {
        "node": ">=8",
        "parcel": "1.x"
    },
    "scripts": {
        "start": "npm run dev",
        "dev": "npm-run-all --parallel dev:build dev:serve",
        "prod": "npm run prod:build && npm run prod:serve",
        "clean": "rimraf dist",
        "dev:build": "npm run clean && cross-env NODE_ENV=development parcel watch src/client/index.js --public-url ./ --out-dir dist/bundles",
        "dev:build:css": "npm run clean && cross-env NODE_ENV=development parcel watch src/client/index.scss --out-dir dist/bundles",
        "dev:serve": "cross-env NODE_ENV=development npx nodemon --exec babel-node src/server test src/server",
        "prod:build": "npm run clean && npm run prod:copy-files && npm run prod:build-server && npm run prod:build-client ",
        "prod:build-client": "cross-env NODE_ENV=production parcel build src/client/index.js --public-url ./ --out-dir dist/bundles --detailed-report",
        "prod:build-server": "cross-env npx babel src/server --out-dir dist/server --ignore test.js",
        "prod:copy-files": "cross-env npx copyfiles -u 1 'src/static/**/*' dist && cross-env npx copyfiles -u 1 'src/client/utils/*.json' dist",
        "prod:serve": "cross-env NODE_ENV=production node dist/server",
        "test": "npm run lint && npm run test:unit && npm run test:e2e",
        "lint": "npx eslint --ext jsx,js,json src test",
        "test:unit": "npx jest -u --config test/unit-test/jest.config.js",
        "test:e2e": "npm run prod:build && cross-env NODE_ENV=production node-while -s dist/server/index.js -r \"node node_modules/cypress/bin/cypress run\"",
        "test:e2e:dev": "npx cypress open --env env=production",
        "test:watch": "npm-watch",
        "test:quick": "npm run lint && npm run test:unit",
        "todo": "npx fixme -i 'node_modules/**' -i '.git/**' -i 'dist/**' --skip note"
    },
    "browser": {
        "./src/client/codeSplitMappingsSync.js": "./src/client/codeSplitMappingsAsync.js"
    },
    "watch": {
        "test:quick": {
            "patterns": [
                "src",
                "test"
            ],
            "extensions": "js,jsx,json",
            "quiet": false
        }
    },
    "postcss": {
        "modules": false,
        "plugins": {
            "autoprefixer": {
                "browsers": [
                    ">1%",
                    "last 4 versions",
                    "Firefox ESR",
                    "not ie < 11"
                ],
                "flexbox": "no-2009"
            }
        }
    },
    "nodemonConfig": {
        "ignore": [
            "src/*",
            "dist/*",
            ".cache/*"
        ]
    },
